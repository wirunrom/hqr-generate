{
  "name": "hqr-generate",
  "version": "0.1.13",
  "description": "Stable black/white QR code generator (PNG data URL) powered by Rust + WASM",
  "type": "module",
  "main": "./index.node.js",
  "module": "./index.browser.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "browser": "./index.browser.js",
      "default": "./index.node.js"
    }
  },
  "files": [
    "pkg/**",
    "index.browser.js",
    "index.node.js",
    "index.d.ts",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "wasm-pack build --target bundler",
    "clean": "rm -rf pkg",
    "prepack": "npm run clean && npm run build && rm -f pkg/.gitignore"
  },
  "keywords": [
    "qrcode",
    "qr",
    "wasm",
    "rust"
  ],
  "license": "MIT"
}